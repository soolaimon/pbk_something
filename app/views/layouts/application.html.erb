<!DOCTYPE html>
<html>
<head>
  <title>PbkSomething</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if flash[:notice] %>
    <p><%= flash[:notice] %></p>
  <% elsif flash[:alert] %>
    <p><%= flash[:alert] %></p>
  <% end %>
<p class="notice"></p>
<%= yield %>

</body>
</html>
